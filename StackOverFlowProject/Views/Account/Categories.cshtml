@model List<stackOverflow.ViewModels.CategoriesViewModel>
@{
    ViewBag.Title="Category";
}

<div class="row">
    <div class="col-sm-12">
        <h4>Categories</h4>
        <ul class="list-group list-group-flush" >
            @foreach(var Category in Model)
            {
                <li class="list-group-item list-group-item-action">@Category.CategoryID . @Category.CategoryName</li>
            }
        </ul>
    </div>

</div>


