@model stackOverflow.ViewModels.QuestionViewModel
<tr>
    <td><span class="badge badge-secondary">@Model.VotesCount </span> Votes</td>
    <td><span class="badge badge-secondary">@Model.AnswerCount </span> Answer</td>
    <td><span class="badge badge-secondary"> @Model.ViewsCount </span> Count</td>
    <td class="text-dark">
        <a class="text-primary" href="/questions/View/@Model.QuestionID"> @Model.QuestionName </a>
        <br />
        <span class="badge badge-info"> @Model.Category.CategoryName </span>
    </td>
    <td>
        @if (Convert.ToInt32(Session["CurrentUserID"]) != Model.UserID)
        {
            @:by @Model.User.Name


        }
        else
        {
            @:By you
        }
    </td>
    <td> on @Model.QuestionDateAndTime</td>
</tr>
